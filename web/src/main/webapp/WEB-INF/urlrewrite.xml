<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN" "http://www.tuckey.org/res/dtds/urlrewrite3.1.dtd">
<urlrewrite>
    <rule>
        <note>Remove trailing slash from all experiments</note>
        <from>/experiments/(.*)/$</from>
        <to>/experiments/$1</to>
    </rule>
    <rule>
        <from>/das/s4(/)?$</from>
        <to>/resources/das/s4.xml</to>
    </rule>
    <rule>
        <from>/das/s4/types(/)?$</from>
        <to>/resources/das/types.xml</to>
    </rule>
</urlrewrite>