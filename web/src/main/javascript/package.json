{
  "name": "expression-atlas-webpack-bundles",
  "version": "1.0.0",
  "description": "Expression Atlas JavaScript Webpack bundles ",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "rm -rf ../webapp/resources/js-bundles/* && webpack --progress --colors --watch --debug --output-pathinfo --display-chunks --display-modules --devtool source-map",
    "dev-min": "rm -rf ../webapp/resources/js-bundles/* && webpack --progress --colors --optimize-minimize --output-pathinfo --display-chunks --display-modules",
    "ci": "rm -rf ../webapp/resources/js-bundles/* && ./install-dependencies.sh && webpack --debug --output-pathinfo --display-chunks --display-modules --devtool source-map",
    "prod": "rm -rf ../webapp/resources/js-bundles/* && ./install-dependencies.sh && webpack --debug --optimize-minimize --output-pathinfo --display-chunks --display-modules"
  },
  "author": "Elisabet Barrera <barrera@ebi.ac.uk> and Alfonso Mu√±oz-Pomer <amunoz@ebi.ac.uk>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "css-loader": "^0.15.4",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "image-webpack-loader": "^1.6.1",
    "imagemin": "^3.2.0",
    "imports-loader": "^0.6.4",
    "jsx-loader": "^0.12.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.1",
    "wolfy87-eventemitter": "^4.2.11"
  },
  "dependencies": {
    "URIjs": "^1.16.0",
    "eve": "^0.4.2",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.10.5",
    "jquery.browser": "0.0.8",
    "react": "^0.12.2"
  }
}
