<configuration experimentType="microarray_1colour_mrna_differential">
    <analytics>
        <array_design>A-AFFY-2</array_design>
        <assay_groups>
            <assay_group id="g1" label="12-oxo-phytodienoic acid; Col-0">
                <assay>Arabidopsis plant (Col-0), 4h_response to OPDA_rep1</assay>
                <assay>Arabidopsis plant (Col-0), 4h_response to OPDA_rep2</assay>
                <assay>Arabidopsis plant (Col-0), 4h_response to OPDA_rep3</assay>
            </assay_group>
            <assay_group id="g2" label="none; Col-0">
                <assay>Arabidopsis plant (Col-0), 4h_control_rep3</assay>
                <assay>Arabidopsis plant (Col-0), 4h_control_rep2</assay>
                <assay>Arabidopsis plant (Col-0), 4h_control_rep1</assay>
            </assay_group>
            <assay_group id="g3" label="phytoprostane; Col-0">
                <assay>Arabidopsis plant (Col-0), 4h_response to phytoprostane A1_rep1</assay>
                <assay>Arabidopsis plant (Col-0), 4h_response to phytoprostane A1_rep3</assay>
                <assay>Arabidopsis plant (Col-0), 4h_response to phytoprostane A1_rep2</assay>
            </assay_group>
            <assay_group id="g4" label="12-oxo-phytodienoic acid; tga2;tga5;tga6 mutant">
                <assay>Arabidopsis plant (tga2-5-6), 4h_response to OPDA_rep3</assay>
                <assay>Arabidopsis plant (tga2-5-6), 4h_response to OPDA_rep2</assay>
                <assay>Arabidopsis plant (tga2-5-6), 4h_response to OPDA_rep1</assay>
            </assay_group>
            <assay_group id="g5" label="none; tga2;tga5;tga6 mutant">
                <assay>Arabidopsis plant (tga2-5-6), 4h_control_rep3</assay>
                <assay>Arabidopsis plant (tga2-5-6), 4h_control_rep1</assay>
                <assay>Arabidopsis plant (tga2-5-6), 4h_control_rep2</assay>
            </assay_group>
            <assay_group id="g6" label="phytoprostane; tga2;tga5;tga6 mutant">
                <assay>Arabidopsis plant (tga2-5-6), 4h_response to phytoprostane A1_rep3</assay>
                <assay>Arabidopsis plant (tga2-5-6), 4h_response to phytoprostane A1_rep2</assay>
                <assay>Arabidopsis plant (tga2-5-6), 4h_response to phytoprostane A1_rep1</assay>
            </assay_group>
        </assay_groups>
        <contrasts>
            <contrast id="g1_g4">
                <name>'tga2;tga5;tga6 mutant; 12-oxo-phytodienoic acid' vs 'Col-0; 12-oxo-phytodienoic acid'</name>
                <reference_assay_group>g1</reference_assay_group>
                <test_assay_group>g4</test_assay_group>
            </contrast>
            <contrast id="g2_g5">
                <name>'tga2;tga5;tga6 mutant; none' vs 'Col-0; none'</name>
                <reference_assay_group>g2</reference_assay_group>
                <test_assay_group>g5</test_assay_group>
            </contrast>
            <contrast id="g3_g6">
                <name>'tga2;tga5;tga6 mutant; phytoprostane' vs 'Col-0; phytoprostane'</name>
                <reference_assay_group>g3</reference_assay_group>
                <test_assay_group>g6</test_assay_group>
            </contrast>
        </contrasts>
    </analytics>
</configuration>
