<configuration experimentType="microarray_1colour_mrna_differential">
    <analytics>
        <array_design>
            A-AFFY-2
        </array_design>
        <assay_groups>
            <assay_group id="g1">
                <assay>noneHS_rep1</assay>
                <assay>noneHS_rep2</assay>
                <assay>noneHS_rep3</assay>
            </assay_group>
            <assay_group id="g2">
                <assay>HS_rep1</assay>
                <assay>HS_rep2</assay>
                <assay>HS_rep3</assay>
			</assay_group>
			<assay_group id="g3">
				<assay>proline_rep1</assay>
				<assay>proline_rep2</assay>
				<assay>proline_rep3</assay>
			</assay_group>
			<assay_group id="g4">
                <assay>AZC_rep1</assay>
                <assay>AZC_rep2</assay>
                <assay>AZC_rep3</assay>
            </assay_group>
            <assay_group id="g5">
				<assay>DMF_rep1</assay>
				<assay>DMF_rep2</assay>
				<assay>DMF_rep3</assay>
			</assay_group>
			<assay_group id="g6">
				<assay>tunicamycin_rep1</assay>
				<assay>tunicamycin_rep2</assay>
				<assay>tunicamycin_rep3</assay>
				<assay>tunicamycin_rep4</assay>
				<assay>tunicamycin_rep5</assay>
			</assay_group>
		</assay_groups>
		<contrasts>
            <contrast id="g1_g2">
                <name>'37 degrees celsius' vs '20 degrees celsius'</name>
                <reference_assay_group>g1</reference_assay_group>
                <test_assay_group>g2</test_assay_group>
            </contrast>
		<contrast id="g3_g4">
                	<name>'L-azetidine-2-carboxylic acid' vs 'L-proline'</name>
                	<reference_assay_group>g3</reference_assay_group>
                	<test_assay_group>g4</test_assay_group>
            	</contrast>
			<contrast id="g5_g6">
				<name>'tunicamycin' vs 'dimethylformamide'</name>
				<reference_assay_group>g5</reference_assay_group>
				<test_assay_group>g6</test_assay_group>
			</contrast>
		</contrasts>
    </analytics>
</configuration>
