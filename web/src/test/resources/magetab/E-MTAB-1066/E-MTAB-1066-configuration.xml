<configuration experimentType="1color_microarray_mrna_differential">
    <analytics>
        <array_design>
            A-AFFY-35
        </array_design>
        <assay_groups>
            <assay_group id="g1">
                <assay>C1</assay>
                <assay>C3</assay>
                <assay>C2</assay>
            </assay_group>
            <assay_group id="g2">
                <assay>WT3</assay>
                <assay>WT1</assay>
                <assay>WT2</assay>
            </assay_group>
            <assay_group id="g3">
                <assay>K3</assay>
                <assay>K2</assay>
                <assay>K1</assay>
            </assay_group>
        </assay_groups>
        <contrasts>
            <contrast id="g2_g1">
                <name>genotype:'cycC mutant' vs 'wild type'</name>
                <reference_assay_group>g2</reference_assay_group>
                <test_assay_group>g1</test_assay_group>
            </contrast>
            <contrast id="g2_g3">
                <name>genotype:'cdk8 mutant' vs 'wild type'</name>
                <reference_assay_group>g2</reference_assay_group>
                <test_assay_group>g3</test_assay_group>
            </contrast>
        </contrasts>
    </analytics>
</configuration>
