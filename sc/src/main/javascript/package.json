{
  "name": "sc-atlas-webpack-bundles",
  "version": "1.0.0",
  "description": "Single-Cell Atlas JavaScript Webpack bundles ",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "webpack --progress --colors --watch --debug --output-pathinfo --display-chunks --display-modules --devtool source-map",
    "dev-min": "webpack --progress --colors --optimize-minimize --output-pathinfo --display-chunks --display-modules",
    "ci": "webpack --debug --output-pathinfo --display-chunks --display-modules --devtool source-map",
    "prod": "NODE_ENV=production webpack --debug --optimize-minimize --output-pathinfo --display-chunks --display-modules"
  },
  "author": "Wojciech Bażant <wbazant@ebi.ac.uk> and Alfonso Muñoz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "license": "Apache 2.0",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.6.3",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "reference-plot": "latest",
    "react": "15.3.2",
    "react-dom": "15.3.2"
  }
}
